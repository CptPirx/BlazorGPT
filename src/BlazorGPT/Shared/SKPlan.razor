@if (Plan != null)
{
    <RadzenTree Expand=@OnExpand Collapse=@OnCollapse>
        <RadzenTreeItem Text="">
            <ChildContent>
                <pre>
@Plan
    </pre>
            </ChildContent>
            <Template>
                <b>Plant</b> 
                <RadzenIcon Icon="plan"></RadzenIcon>
            </Template>
        </RadzenTreeItem>
    </RadzenTree>
}

@code {

    [Parameter]
    public string? Plan { get; set; }

    private void OnExpand()
    {
    }

    private void OnCollapse()
    {
    }

}